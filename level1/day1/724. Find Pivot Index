class Solution {
    public int pivotIndex(int[] nums) {
        int pI = 0, ls = 0, rs=0;
        for(int i=1; i< nums.length;i++){
            rs=rs+nums[i];
        }
        for(int i =0; i< nums.length;i++){
            if(i != 0 ){rs =  rs - nums[i];}
            pI=i;
            if(ls == rs){
                return i;
            }else{
                ls = ls + nums[i];
            }
        }
        return -1;
    }
}
